<% @events.each do |event| %>

  <div class="container">
    <div class="row event-box">
      <div class="col-md-6 col-md-offset-3">
        <h2> <%= event.title %> </h2>
        <h3> <%= event.venue_name %> </h3>
        <h4> <%= event.venue_address %> </h4>
        <p>  <%= link_to "Details", event.url, class: "btn btn-lg btn-default", target: "_blank" %> </p>
      </div>
    </div>
  </div>

<% end %>
